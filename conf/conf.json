{
  "Common": {
    "HttpAddr": "127.0.0.1:2369",
    "Log": {
      "LogPath": "./log",
      "LogName": "crawler"
    },
    "Download": {
      "OutPath": "./download",
      "ChanSize": 2048,
      "GoroutineCount": 4
    },
    "Analysis": {
      "ChanSize": 2048,
      "GoroutineCount": 4
    }
  },

  "Update": {
    "Route": "/update",
    "Urls": [
      {
        "Index": "http://www.silisili.me",
        "Selectors": [
          {
            "Dom": ".time_con li",
            "Exec": [
              {
                "Dom": "p",
                "Attr": ""
              },
              {
                "Dom": "img",
                "Attr": "src"
              }
            ]
          }
        ]
      }
    ]
  },

  "Search": {
    "Route": "/search",
    "Urls": [
      {
        "Index": "http://www.silisili.me",
        "Selectors": [
          {
            "Type": "attr",
            "Dom": ".time_con li img",
            "Attr": "src"
          },{
            "Type": "attr",
            "Dom":"",
            "Attr": ""
          }
        ]
      }
    ]
  }



}